    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "highlight.js": "^11.11.1",
    "lucide-svelte": "^0.561.0",
    "marked": "^17.0.1"
  }
}
